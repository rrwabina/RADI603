\item{Determine the angle between the following vectors.}

\begin{enumerate}

\item
$ \!
\vec{a} = (3, -4, -1)$ and $\vec{b} = (0, 5, 2) \\ 
\text{Since $a$ or $b$ are non-zero vectors, then $\vec{a} \cdot \vec{b} &= \lVert \vec{a} \rVert \cdot \lVert \vec{b} \rVert \cdot \cos{\theta}$}
\hfill \break\hfill \break 
\begin{aligned}[t]
    \vec{a} \cdot \vec{b} &= \lVert \vec{a} \rVert \cdot \lVert \vec{b} \rVert \cdot \cos{\theta} \\
    (3, -4, -1) \cdot (0, 5, 2) &= \lVert (3, -4, -1)  \rVert \cdot \lVert (0, 5, 2)  \rVert \cdot \cos{\theta} \\
    (3)(0) + (-4)(5) + (-1)(2)  &= \left[ \sqrt{3^2 + (-4)^2 + (-1)^2} \cdot \sqrt{0^2 + (5)^2 + (2)^2} \right] \cdot \cos{\theta} \\
    -22 &= 27.45906043549196 \cdot \cos{\theta} \\
    \frac{-22}{27.45906043549196} &= \cos{\theta} \\
    \theta &= \cos^{-1}{\frac{-22}{27.45906043549196}} \\
    \theta &= 2.5 \text{\hspace{2mm} (radians)} \\
    \theta &= 2.5 \cdot \frac{180}{\pi} \\
    \theta &= 143.24 \text{\hspace{2mm} (degrees)}
    
\end{aligned}
$
\hfill \break \hfill \break 


\item
$ \!
\vec{a} = (1, 1, 1)$ and $\vec{b} = (3, 3, 3) \\ 
\text{Since $a$ or $b$ are non-zero vectors, then $\vec{a} \cdot \vec{b} &= \lVert \vec{a} \rVert \cdot \lVert \vec{b} \rVert \cdot \cos{\theta}$}
\hfill \break\hfill \break 
\begin{aligned}[t]
    \vec{a} \cdot \vec{b} &= \lVert \vec{a} \rVert \cdot \lVert \vec{b} \rVert \cdot \cos{\theta} \\
    (1, 1, 1) \cdot (3, 3, 3) &= \lVert (1, 1, 1)  \rVert \cdot \lVert (3, 3, 3)  \rVert \cdot \cos{\theta} \\
    (1)(3) + (1)(3) + (1)(3)  &= \left[ \sqrt{1^2 + 1^2 + 1^2} \cdot \sqrt{3^2 + 3^2 + 3^2} \right] \cdot \cos{\theta} \\
    9 &= 9 \cdot \cos{\theta} \\
    \frac{9}{9} &= \cos{\theta} \\
    \theta &= \cos^{-1}{1} \\
    \theta &= 0 \text{\hspace{2mm} (radians)} \\
    \theta &= 0 \cdot \frac{180}{\pi} \\
